{
  "name": "balancy-app-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run prepare",
    "build": "nest build",
    "lint:format": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "lint:fs": "ls-lint",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint:editor": "editorconfig-checker",
    "start:prod": "node dist/main",
    "lint:js": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:type": "npx tsc --noEmit",
    "lint:js:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "npm run lint:format && npm run lint:fs && npm run lint:js && npm run lint:type",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "typeorm": "npm run build && npx typeorm -d dist/database/data-source.js",
    "schema:drop": "npm run typeorm -- schema:drop",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.4.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/platform-socket.io": "^9.4.3",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.1.11",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.3.1",
    "google-auth-library": "^9.14.1",
    "i": "^0.3.7",
    "install": "^0.13.0",
    "joi": "^17.9.2",
    "nestjs-cloudinary": "^2.0.6",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.3",
    "npm": "^10.8.3",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "streamifier": "^0.1.1",
    "typeorm": "^0.3.16",
    "ua-parser-js": "^0.7.39",
    "uuid": "^10.0.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^2.0.0",
    "@nestjs/cli": "^10.0.3",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^9.4.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^28.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/node-schedule": "^2.1.6",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/streamifier": "^0.1.2",
    "@types/supertest": "^2.0.12",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^10.0.0",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "editorconfig-checker": "^5.1.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.5.3"
  }
}
